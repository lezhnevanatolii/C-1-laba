#include <iostream>

bool isPositive(int x) {
    return x > 0;
}

int main() {
    int x;
    
    std::cout << "Введите число: ";
    std::cin >> x;
    
    if (isPositive(x)) {
        std::cout << "x=" << x << "\nрезультат: true" << std::endl;
    } else {
        std::cout << "x=" << x << "\nрезультат: false" << std::endl;
    }
    
    return 0;
}
