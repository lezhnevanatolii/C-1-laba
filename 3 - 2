#include <iostream>
#include <string>

std::string reverseListNums(int x) {
    std::string result = "";
    for (int i = x; i >= 0; i--) {
        result += std::to_string(i);
        if (i > 0) {
            result += " ";
        }
    }
    return result;
}

int main() {
    int number;
    std::cout << "Введите число: ";
    std::cin >> number;
    
    if (std::cin.fail() || number < 0) {
        std::cout << "Ошибка ввода!" << std::endl;
        return 1;
    }
    
    std::string result = reverseListNums(number);
    std::cout << "Результат: " << result << std::endl;
    
    return 0;
}
