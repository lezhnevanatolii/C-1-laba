#include <iostream>


int sumLastNums(int x) {
    int lastDigit = x % 10;             
    int secondLastDigit = (x / 10) % 10; 
    return lastDigit + secondLastDigit;
}

int main() {
    int x;

    std::cout << "Введите число (минимум из двух цифр): ";
    std::cin >> x;

    int result = sumLastNums(x);
    std::cout << "Результат: " << result << std::endl;

    return 0;
}
